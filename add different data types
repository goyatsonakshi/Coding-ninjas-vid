#include <iostream>
using namespace std;

int main() {
    int i = 10;
    char c = 'a';

    int o = i + c;
    char d = i + c;
    cout << o << endl;
    cout << d << endl;
}

// in int o it took it as integer and provided the solution as 107
// but when char d is given then it will read 107 as ascii character value that is k.
